        "columns": [
          {
            "name": "id",
            "type": "VARCHAR(240)"
          },
          {
            "name": "src_id",
            "type": "VARCHAR(240)"
          },
          {
            "name": "src_volgnummer",
            "type": "INT"
          },
          {
            "name": "dst_id",
            "type": "VARCHAR(240)"
          },
          {
            "name": "dst_volgnummer",
            "type": "INT"
          },
          {
            "name": "bronwaarde",
            "type": "VARCHAR(240)"
          }
        ]
      },
      "depends_on": [
        "select_beperkingen"
      ],
      "id": "select_rel_wkpb_bpg_brk_kot_belast_kadastrale_objecten"
    }
  ]
}