      "source": "src",
      "query": "data/sql/wkpb/select.beperkingen.sql",
      "query_src": "file",
      "destination_table": {
        "name": "wkpb_prep.beperkingen",
        "create": true,
        "columns": [
          {
            "name": "id",
            "type": "VARCHAR(240)"
          },
          {
            "name": "src_id",
            "type": "VARCHAR(240)"
          },
          {
            "name": "src_volgnummer",
            "type": "INT"
          },
          {
            "name": "bronwaarde",
            "type": "VARCHAR(240)"
          }
        ]
      },
