        "name": "brk.bestand",
        "create": true,
        "columns": [
          {
            "name": "brk_bsd_toestandsdatum",
            "type": "TIMESTAMP"
          },
          {
            "name": "omschrijving",
            "type": "VARCHAR(120)"
          }
        ]
      },
